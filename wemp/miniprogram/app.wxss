/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 25% 0;
  box-sizing: border-box;
} 

.btn {    
  display: flex;
  justify-content: center;

  width: 80%;
  height: 95rpx;
  line-height: 95rpx;
  
  font-size: 1rem;
  color: white;
  border-radius: 7.5rpx;
  text-align: center;
  background-color: #17a586;
}

@keyframes breath {
	0% {
		transform: scale(1, 1);
	}
	50% {
		transform: scale(0.9, 0.9);
	}
	100% {
		transform: scale(1, 1);
	}
}

